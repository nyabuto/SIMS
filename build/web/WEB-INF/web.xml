<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>dbsetup</servlet-name>
        <servlet-class>Db.dbsetup</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Db.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Db.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>Db.register</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_profile</servlet-name>
        <servlet-class>Db.load_profile</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loadform</servlet-name>
        <servlet-class>SIMS.loadform</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_county</servlet-name>
        <servlet-class>SIMS.load_county</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_subcounty</servlet-name>
        <servlet-class>SIMS.load_subcounty</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>load_facility</servlet-name>
        <servlet-class>SIMS.load_facility</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>save_assessment_form</servlet-name>
        <servlet-class>SIMS.save_assessment_form</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dbsetup</servlet-name>
        <url-pattern>/dbsetup</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_profile</servlet-name>
        <url-pattern>/load_profile</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loadform</servlet-name>
        <url-pattern>/loadform</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_county</servlet-name>
        <url-pattern>/load_county</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_subcounty</servlet-name>
        <url-pattern>/load_subcounty</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>load_facility</servlet-name>
        <url-pattern>/load_facility</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>save_assessment_form</servlet-name>
        <url-pattern>/save_assessment_form</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
